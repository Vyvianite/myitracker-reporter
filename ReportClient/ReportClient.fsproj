<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>ReportClient</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OtherFlags>--sig:GeneratedSigs.fsi</OtherFlags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="OSType.fs" />
    <Compile Include="Logger.fs" />
    <Compile Include="Utilities.fs" />
    <Compile Include="LemonCypher.fs" />
    <Compile Include="Config.fs" />
    <Compile Include="Reporter.fs" />
    <Compile Include="HttpApi.fs" />
    <Compile Include="Commands.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="3.1.1" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="CliWrap" Version="3.2.2" />
    <PackageReference Include="FSharp.Core" Version="5.0.0" />
  </ItemGroup>
</Project>